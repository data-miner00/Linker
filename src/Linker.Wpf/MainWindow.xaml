<Window x:Class="Linker.Wpf.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Linker.Wpf"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Label x:Name="lblLogo" Content="Linker" HorizontalAlignment="Left" Margin="30,20,0,0" VerticalAlignment="Top" FontSize="24"/>
        <ListBox x:Name="sampleList" Margin="30,132,105,112"/>
        <Button x:Name="btnLoadArticle" Content="Load Articles" HorizontalAlignment="Left" Margin="30,84,0,0" VerticalAlignment="Top" Click="btnLoadArticle_Click"/>
        <Button x:Name="btnLoadWebsites" Content="Load Websites" HorizontalAlignment="Left" Margin="119,84,0,0" VerticalAlignment="Top" Click="btnLoadWebsites_Click"/>
        <Button x:Name="btnLoadYoutube" Content="Load Youtube" HorizontalAlignment="Left" Margin="219,84,0,0" VerticalAlignment="Top" Click="btnLoadYoutube_Click"/>

    </Grid>
</Window>
