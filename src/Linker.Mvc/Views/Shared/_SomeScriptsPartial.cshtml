<script src="~/lib/loaded.js"></script>
<environment names="Development">
    <script src="~/lib/jquery/jquery.min.js"></script>
    <script src="~/lib/signalr/signalr.min.js"></script>
</environment>
<environment names="Staging,Production">
    <script
        src="https://www.unpkg.com/jquery@3.7.1/dist/jquery.min.js"
        asp-fallback-src="~/lib/jquery/jquery.min.js"
        asp-fallback-test="window.jQuery"
    ></script>
    <script
        src="https://unpkg.com/@@microsoft/signalr@5.0.11/dist/browser/signalr.min.js"
        asp-fallback-src="~/lib/signalr/signalr.min.js"
        asp-fallback-test="window.signalR"
    ></script>
</environment>
